<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
	<id>dist-trial</id>
	<formats>
		<format>zip</format>
		<format>tar.gz</format>
	</formats>
	<fileSets>
		<fileSet>
			<directory>src/main/resources/apidocs/</directory>
			<outputDirectory>guides/javadoc</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/scripts/</directory>
			<outputDirectory>bin</outputDirectory>
			<fileMode>777</fileMode>
			<lineEnding>unix</lineEnding>
			<includes>
				<include>*.sh</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/scripts/</directory>
			<outputDirectory>bin</outputDirectory>
			<fileMode>777</fileMode>
			<lineEnding>windows</lineEnding>
			<includes>
				<include>*.bat</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/</directory>
			<outputDirectory>ext-lib</outputDirectory>
			<includes>
				<include>CLASSPATH_INFO.TXT</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/</directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>STREAMHORIZON_README.TXT</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/</directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>LICENCE.TXT</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/</directory>
			<outputDirectory>guides</outputDirectory>
			<includes>
				<include>*.pdf</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/</directory>
			<outputDirectory>log</outputDirectory>
			<includes>
				<include>LOG_README.TXT</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/config/</directory>
			<outputDirectory>config</outputDirectory>
			<fileMode>666</fileMode>
			<includes>
				<include>*.*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/plugins/</directory>
			<outputDirectory>plugins</outputDirectory>
			<includes>
				<include>*.*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/java/</directory>
			<outputDirectory>plugins</outputDirectory>
			<includes>
				<include>*.java</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/</directory>
			<outputDirectory>demo_sales</outputDirectory>
			<fileMode>777</fileMode>
			<includes>
				<include>*.*</include>
			</includes>
			<excludes>
				<exclude>*.sh</exclude>
			</excludes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/</directory>
			<outputDirectory>demo_sales</outputDirectory>
			<fileMode>777</fileMode>
			<lineEnding>unix</lineEnding>
			<includes>
				<include>*.sh</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/examples/</directory>
			<outputDirectory>demo_sales/examples</outputDirectory>
			<includes>
				<include>*.*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/mysql/</directory>
			<outputDirectory>demo_sales/mysql</outputDirectory>
			<includes>
				<include>*.*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/mssql/</directory>
			<outputDirectory>demo_sales/mssql</outputDirectory>
			<includes>
				<include>*.*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/oracle/jdbc-deploy/</directory>
			<outputDirectory>demo_sales/oracle/jdbc-deploy</outputDirectory>
			<includes>
				<include>*.*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/oracle/ext-table-deploy/</directory>
			<outputDirectory>demo_sales/oracle/ext-table-deploy</outputDirectory>
			<includes>
				<include>*.*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/oracle/pipe-deploy/</directory>
			<outputDirectory>demo_sales/oracle/pipe-deploy</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/bin/</directory>
			<outputDirectory>demo_sales/local_cluster_example/bin</outputDirectory>
			<fileMode>777</fileMode>
			<lineEnding>unix</lineEnding>
			<includes>
				<include>*.sh</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/demo_sales/bin/</directory>
			<outputDirectory>demo_sales/local_cluster_example/bin</outputDirectory>
			<lineEnding>windows</lineEnding>
			<includes>
				<include>*.bat</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>target/</directory>
			<outputDirectory>lib</outputDirectory>
			<includes>
				<include>streamhorizon-impl-${project.version}.jar</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/data/</directory>
			<outputDirectory>data</outputDirectory>
			<includes>
				<include>DATA_README.TXT</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/data/</directory>
			<outputDirectory>data/db</outputDirectory>
			<includes>
				<include>DATA_README.TXT</include>
			</includes>
		</fileSet>
	</fileSets>
	<dependencySets>
		<dependencySet>
			<outputDirectory>lib</outputDirectory>
			<fileMode>666</fileMode>
			<includes>
				<include>*:jar:*</include>
			</includes>
			<excludes>
				<exclude>com.threeglav.streamhorizon.bauk:streamhorizon-core</exclude>
				<exclude>com.threeglav.streamhorizon.bauk:streamhorizon-feed-parser</exclude>
				<exclude>com.threeglav.streamhorizon.bauk:streamhorizon-plugins</exclude>
				<exclude>com.threeglav.streamhorizon.bauk:streamhorizon-distribution-packaging</exclude>
			</excludes>
		</dependencySet>
		<dependencySet>
			<outputDirectory>ext-lib</outputDirectory>
			<fileMode>666</fileMode>
			<includes>
				<include>com.threeglav.streamhorizon.bauk:streamhorizon-plugins</include>
			</includes>
		</dependencySet>
	</dependencySets>
</assembly>