<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config>
	<sourceDirectory></sourceDirectory>
	<archiveDirectory>d:/projects/test/archive</archiveDirectory>
	<errorDirectory>d:/projects/test/error</errorDirectory>
	<bulkOutputDirectory></bulkOutputDirectory>
	<properties>
	</properties>
	<connectionProperties>
		<jdbcUrl>jdbc:h2:mem:bauk_test;DB_CLOSE_DELAY=-1</jdbcUrl>
	</connectionProperties>
	<databaseStringLiteral>'</databaseStringLiteral>
	<databaseStringEscapeLiteral>''</databaseStringEscapeLiteral>
	<factFeeds>
		<factFeed name="TEST_Feed" type="full">
			<fileNameMasks>
				<fileNameMask>.*bauk_test</fileNameMask>
			</fileNameMasks>
			<delimiterString>,</delimiterString>
			<header process="no_header" />
			<data process="no_validation">
				<eachLineStartsWithCharacter></eachLineStartsWithCharacter>
				<attributes>
					<attribute name="attr1" />
					<attribute name="attr2" />
					<attribute name="a_in" />
					<attribute name="b_in" />
				</attributes>
			</data>
			<footer process="skip" />
			<bulkLoadDefinition outputType="jdbc">
				<bulkLoadInsert>
					<command type="sql">insert into TEST_FACT(f1,f2,f3,f4) values(?,?,?,'${bulkFileName}')</command>
				</bulkLoadInsert>
				<bulkLoadFormatDefinition>
					<attributes>
						<attribute name="dimension.TEST_DIM" type="int" />
						<attribute name="feed.attr1" type="int" />
						<attribute name="feed.attr2" type="int" />
					</attributes>
				</bulkLoadFormatDefinition>
			</bulkLoadDefinition>
			<threadPoolSizes>
				<feedProcessingThreads>1</feedProcessingThreads>
				<bulkLoadProcessingThreads>1</bulkLoadProcessingThreads>
			</threadPoolSizes>
		</factFeed>
	</factFeeds>
	<dimensions>
		<dimension name="TEST_DIM" type="T1_INSERT_ONLY">
			<localCacheMaxSize>50000</localCacheMaxSize>
			<mappedColumns>
				<mappedColumn name="a_in" naturalKey="true" />
				<mappedColumn name="b_in" naturalKey="true" />
			</mappedColumns>
			<sqlStatements>
				<insertSingle>
					insert into TEST_DIM(a,b) values ('${a}','${b}')
				</insertSingle>
				<selectSurrogateKey>select id from TEST_DIM where a='${a_in}' and b='${b_in}'</selectSurrogateKey>
				<preCacheKeys>select id, a, b from TEST_DIM</preCacheKeys>
			</sqlStatements>
		</dimension>
	</dimensions>
</config>
